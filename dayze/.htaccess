RewriteEngine On
RewriteRule ^$	/today.php	[L]
RewriteRule ^(\d{4})/?$	/today.php?year=$1	[L]
RewriteRule ^(\d{4})/wk(\d{2})?$	/today.php?year=$1&week=$2	[L]
RewriteRule ^(\d{4})/(\d{2})/?$	/today.php?year=$1&month=$2	[L]
RewriteRule ^(\d{4})/(\d{1,2})/(\d{1,2})/?$	/today.php?year=$1&month=$2&day=$3	[L]
